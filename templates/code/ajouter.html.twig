          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Ajouter code promo</h4>
              <p class="card-description"></p>
             {% block body %}
             
             {{form_start(form,{'attr':{'class':'form-horizontal'}})}}            
             <div class="form-group" >
             {{ form_label(form.code, 'Description', {'attr': {'class': 'form-control-label'}}) }}
             {{ form_widget(form.code, {'attr': {'class': 'form-control', 'placeholder': "code promo"}}) }}
             {% if form_errors(form.code) %}
            <div class="alert alert-danger">{{ form_errors(form.code)|join(', ') }}</div>
        {% endif %}
             </div>
            
             <div class="form-group" >
             {{ form_label(form.datedebut, 'date de debut', {'attr': {'class': 'form-control-label'}}) }}
             {{ form_widget(form.datedebut, {'attr': {'class': 'form-control', 'placeholder': 'datedebut'}}) }}
             {% if form_errors(form.datedebut) %}
            <div class="alert alert-danger">{{ form_errors(form.datedebut)|join(', ') }}</div>
        {% endif %}
             </div>
             <div class="form-group" >
             {{ form_label(form.datefin, 'date de fin', {'attr': {'class': 'form-control-label'}}) }}
             {{ form_widget(form.datefin, {'attr': {'class': 'form-control', 'placeholder': 'datefin'}}) }}
             {% if form_errors(form.datefin) %}
            <div class="alert alert-danger">{{ form_errors(form.datefin)|join(', ') }}</div>
        {% endif %}
            </div>
            <div class="form-group">
                {{ form_label(form.reductionassocie, 'reductionassocie', {'attr': {'class': 'form-control-label'}}) }}
                {{ form_widget(form.reductionassocie, {'attr': {'class': 'form-control', 'placeholder': 'reduction'}}) }}
                {% if form_errors(form.reductionassocie) %}
            <div class="alert alert-danger">{{ form_errors(form.reductionassocie)|join(', ') }}</div>
        {% endif %}
             </div>
             
             <button type="submit" class="btn btn-success">Ajouter</button>

             {{ form_end(form) }}

             {% endblock %}
            </div>
          </div>
        </div>


