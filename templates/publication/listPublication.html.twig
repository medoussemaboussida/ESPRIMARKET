<!-- Script JavaScript -->
<script>
    // Fonction pour générer le PDF
    function generatePDF(event) {
        event.preventDefault(); // Empêcher le comportement par défaut du lien

        // Faire une requête GET vers la route Symfony pour générer le PDF
        fetch("{{ path('generate_pdf') }}", {
            method: 'GET',
        })
        .then(response => {
            // Vérifier si la réponse est OK
            if (!response.ok) {
                throw new Error('Une erreur est survenue lors de la génération du PDF.');
            }
            return response.blob(); // Renvoie le contenu du PDF en tant que blob
        })
        .then(blob => {
            // Créer un lien temporaire pour le téléchargement du PDF
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = 'liste_publications.pdf'; // Nom du fichier PDF
            document.body.appendChild(a);
            a.click();
            window.URL.revokeObjectURL(url); // Libérer l'URL
        })
        .catch(error => {
            console.error('Erreur lors du téléchargement du PDF:', error);
            alert('Une erreur est survenue lors de la génération du PDF.');
        });
    }

    // Ajouter un écouteur d'événement au lien PDF
    const pdfLink = document.getElementById("generatePdfLink");
    pdfLink.addEventListener("click", generatePDF);
</script>

