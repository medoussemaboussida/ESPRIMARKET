                        <img src="{{ asset('images/' ~ publication.imagePublication) }}" alt="{{ publication.titrePublication }}" style="width: 500px; height: 300px; object-fit: cover; border-radius: 5px;">
                        <div class="carousel-caption" style="position: absolute; bottom: 20px; left: 0; right: 0; text-align: center; background-color: rgba(0, 0, 0, 0.7); color: #fff; padding: 10px;">
                            <h5>{{ publication.titrePublication }}</h5>
                            <h3 class="comment-count-title">
                                {% if publication.commentairesCount is defined %}
                                    {{ publication.commentairesCount }} {% if publication.commentairesCount > 1 %}Comments{% else %}Comment{% endif %}
                                {% else %}
                                    0 Comments
                                {% endif %}
                            </h3>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>


